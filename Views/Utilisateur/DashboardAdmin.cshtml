<div class="dash-adm">
    <h2>Comptes utilisateur</h2>
    <div class="dash-usrlist">
        <table class="table table-info table-striped table-hover">
            <tr>
                <th>Nom</th>
                <th>Prenom</th>
                <th>adresse mail</th>
                <th>Role</th>
                <th>nombre d'annonce</th>
                <th>Modifer</th>
            </tr>
            @foreach (var utilisateur in Model.Utilisateurs)
            {
                <tr>
                    <td class="table-info user-list">@utilisateur.InfosPersonnelle.Prenom</td>
                    <td>@utilisateur.InfosPersonnelle.Nom</td>
                    <td class="table-info">@utilisateur.Compte.email</td>
                    <td>@utilisateur.Compte.Role</td>
                    <td class="table-info">@utilisateur.Compte.nombreAnnonce</td>
                    <td class="table-info"><a href="utilisateur/modifierutilisateur/@utilisateur.Id">Edit</a></td>
                </tr>
            }
        </table>
    </div>
</div>